@import "colorui/main.wxss";
@import "colorui/icon.wxss";

.blank-view {
    height: 118rpx;
    width: 100%;
    margin-top: 30rpx;
}
.bottom-view {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    background-color: white;
    height: 118rpx;
    padding: 20rpx;
    border-top: 2rpx solid #f0f0f0;
    z-index: 99;
}